{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "clean": "rm -rf ./dist && rm -f tsconfig.tsbuildinfo",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migration:create": "typeorm migration:create -d src/migrations -n",
    "migration:run": "npm run build && node dist/migration",
    "seed": "docker compose up -d && node ./dist/fixtures/loader",
    "seed:build": "npm run clean && npm run build && npm run seed:custom",
    "seed:custom": "node ./dist/fixtures/loader",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@date-io/date-fns": "2.10.11",
    "@nestjs-query/core": "0.27.0",
    "@nestjs-query/query-graphql": "0.27.0",
    "@nestjs-query/query-typeorm": "0.27.0",
    "@nestjs/common": "7.6.18",
    "@nestjs/core": "7.6.18",
    "@nestjs/graphql": "8.0.2",
    "@nestjs/platform-express": "7.6.18",
    "@nestjs/typeorm": "7.1.5",
    "apollo-server-express": "2.25.2",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dataloader": "2.0.0",
    "date-fns": "2.22.1",
    "graphql": "15.5.1",
    "graphql-subscriptions": "1.2.1",
    "pg": "8.6.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "typeorm": "0.2.34"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.18",
    "@types/express": "4.17.12",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.4",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "prettier": "2.3.2",
    "supertest": "6.1.3",
    "ts-jest": "26.5.6",
    "ts-loader": "8.3.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typeorm-fixtures-cli": "1.9.1",
    "typescript": "4.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext ts"
    ],
    "*.{md,html,json,js}": [
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test-config",
      ".dto.ts",
      "loader.ts",
      "ormconfig.ts",
      "db-conn.ts",
      "main.ts",
      ".entity.ts",
      "migration.ts",
      "jestGlobalMocks.ts",
      ".module.ts",
      ".mock.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
